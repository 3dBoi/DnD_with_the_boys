<AnchorPane id="AnchorPane" fx:id="nameMenuePane" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.FXMLNameMenueController">
   <children>
      <Label fx:id="nameMenueLabelMain" layoutX="370.0" layoutY="143.0" text="WHAT IS YOUR NAME?">
         <font>
            <Font name="Noto Mono" size="50.0" />
         </font>
      </Label>
      <TextField fx:id="nameMenueName" layoutX="542.0" layoutY="262.0" onAction="#handleNameMenueName" prefHeight="25.0" prefWidth="197.0" />
      <Button fx:id="nameMenueBtnAccept" layoutX="614.0" layoutY="307.0" mnemonicParsing="false" onAction="#handleNameMenueBtnAccept" text="Accept" />
      <Button fx:id="nameMenueBtnReturn" layoutX="14.0" layoutY="761.0" mnemonicParsing="false" onAction="#handleNameMenueBtnReturn" text="Return" />
   </children>
</AnchorPane>
